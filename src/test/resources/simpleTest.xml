<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="suite01" parallel="false">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>
    <test name="CryptoCompareTests" enabled="false">
        <packages>
            <package name="com.epam.tat21.crypto.ui.tests.*"></package>
            <package name="com.epam.tat21.crypto.api.tests.*"></package>
        </packages>
    </test>

    <test name="mobile portfolio">
        <classes>
            <class name="com.epam.tat21.crypto.mobile.tests.UserPortfolioMobileTest"></class>
        </classes>
    </test>

    <test name="Mobile CryptoCompareTests" enabled="false">
        <packages>
            <package name="com.epam.tat21.crypto.mobile.tests.*"></package>
        </packages>
    </test>
</suite>