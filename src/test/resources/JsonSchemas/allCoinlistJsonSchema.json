{  
   "$schema":"http://json-schema.org/draft-07/schema#",
   "comment":"The all coinlist endpoint schema",
   "type":"object",
   "properties":{  
      "Response":{  
         "type":"string",
         "pattern":"^([A-Za-z\\s])+$"
      },
      "Message":{  
         "type":"string"
      },
      "Data":{  
         "type":"object",
         "patternProperties":{  
            "([A-Z]|[0-9])":{  
               "type":"object",
               "properties":{  
                  "Id":{  
                     "type":"string",
                     "pattern":"^([0-9])+$"
                  },
                  "Url":{  
                     "type":"string"
                  },
                  "ImageUrl":{  
                     "type":"string"
                  },
                  "Name":{  
                     "type":"string"
                  },
                  "Symbol":{  
                     "type":"string"
                  },
                  "CoinName":{  
                     "type":"string"
                  },
                  "FullName":{  
                     "type":"string"
                  },
                  "Algorithm":{  
                     "type":"string"
                  },
                  "ProofType":{  
                     "type":"string"
                  },
                  "FullyPremined":{  
                     "type":"string"
                  },
                  "TotalCoinSupply":{  
                     "type":"string"
                  },
                  "BuiltOn":{  
                     "type":"string"
                  },
                  "SmartContractAddress":{  
                     "type":"string"
                  },
                  "PreMinedValue":{  
                     "type":"string"
                  },
                  "TotalCoinsFreeFloat":{  
                     "type":"string"
                  },
                  "SortOrder":{  
                     "type":"string"
                  },
                  "Sponsored":{  
                     "type":"boolean"
                  },
                  "IsTrading":{  
                     "type":"boolean"
                  },
                  "TotalCoinsMined":{  
                     "type":"number"
                  },
                  "BlockNumber":{  
                     "type":"integer"
                  },
                  "NetHashesPerSecond":{  
                     "type":"number"
                  },
                  "BlockReward":{  
                     "type":"number"
                  },
                  "BlockTime":{  
                     "type":"integer"
                  }
               },
			"additionalProperties":false            
			}
         }
      },
      "BaseImageUrl":{  
         "type":"string",
         "format":"uri"
      },
      "BaseLinkUrl":{  
         "type":"string",
         "format":"uri"
      },
      "RateLimit":{  

      },
      "HasWarning":{  
         "type":"boolean"
      },
      "Type":{  
         "type":"integer"
      }
   },
   "additionalProperties":false
}